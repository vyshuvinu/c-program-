#include <stdio.h>

//Compiler version gcc  6.3.0

int main()
{
  /*input name*/
 char name[]="Gouri";
 printf( "Name of the student= %s\n", name);
 float eng, maths, bio;
 float total, average;
 /*input marks*/
 printf("Enter the marks scored in 3 subjects= \n");
 scanf("%f%f%f", &eng,&maths, &bio);
 /*total, average*/
   total=eng+maths+bio;
   average =total/3;
   /*display total, average */
     printf("total marks = %f\n", total);
     printf("average marks= %f\n", average);
 
  /**/
  return 0;
}
